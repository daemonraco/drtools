import { Task } from 'drtools';

class <%= properClassName %>Task extends Task {
    //
    // Public methods.
    public description(): string {
        return '@TODO no description provided yet';
    }
    public name(): string {
        return '<%= properName %>';
    }
    public async run(): Promise<void> {
        console.log(`Running '<%= properClassName %>Task': ${new Date()}`);
    }
    //
    // Protected methods.
    protected load(): void {
        this._interval = <%= interval %>;
        this._runAtStart = <%= runOnStart %>;
    }
}

export = new <%= properClassName %>Task();
