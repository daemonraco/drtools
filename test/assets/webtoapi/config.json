{
    "cachePath": "test/tmp/webtoapi/cache",
    "endpoints": [
        {
            "name": "main",
            "url": "http://localhost:3005/webtoapi-test/:title:",
            "fields": [
                {
                    "name": "title",
                    "path": "h1.main-title",
                    "parser": "trim-text"
                },
                {
                    "name": "fruits-origin",
                    "path": "#fruits",
                    "parser": "attr",
                    "parserParams": "data-origin"
                },
                {
                    "name": "fruits",
                    "path": "#fruits li",
                    "fields": [
                        {
                            "name": "name",
                            "path": ".name",
                            "parser": "trim-text"
                        },
                        {
                            "name": "count",
                            "path": ".count",
                            "parser": "trim-text"
                        }
                    ]
                }
            ]
        }
    ],
    "routes": [
        {
            "path": "/main/:title",
            "endpoint": "main",
            "map": {
                "title": "title"
            }
        }
    ]
}